<%= simple_form_for(@entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :description %>
    <%= f.input :airtemp, :label => "Air Temp" %>
    <%= f.input :watertemp, :label => "Water Temp" %>
    <%= f.input :conditions %>
    <%= f.input :speciessought, :label => "Species Sought" %>
    <%= f.input :speciescaught, :label => "Species Caught" %>
    <%= f.input :flypattern, :label => "Fly Pattern" %>
    <%= f.input :pressure %>
    <%= f.input :rating, :collection => 1..5 %>
    
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
